    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "rimraf": "^5.0.1",
    "svelte": "^4.1.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-is"
      ]
    }
