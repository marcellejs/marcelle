{
  "name": "@marcellejs/backend",
  "description": "A real-time server for Marcelle applications",
  "version": "0.6.1",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.16.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": false
  },
  "bin": {
    "marcelle-backend": "./bin/backend.cjs"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "build": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run build && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.8",
    "@feathersjs/authentication-client": "^5.0.8",
    "@feathersjs/configuration": "^5.0.8",
    "@feathersjs/errors": "^5.0.8",
    "@feathersjs/feathers": "^5.0.8",
    "@feathersjs/koa": "^5.0.8",
    "@feathersjs/mongodb": "^5.0.8",
    "@feathersjs/schema": "^5.0.8",
    "@feathersjs/socketio": "^5.0.8",
    "@feathersjs/transport-commons": "^5.0.8",
    "@koa/multer": "^3.0.2",
    "feathers-hooks-common": "^8.0.0",
    "mongodb": "^5.7.0",
    "multer": "1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.6",
    "@feathersjs/rest-client": "^5.0.8",
    "@feathersjs/typebox": "^5.0.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.8",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}