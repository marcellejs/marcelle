{
  "name": "~TODO~",
  "description": "A Marcelle Application with SvelteKit",
  "version": "0.0.0",
  "keywords": ["marcelle"],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
