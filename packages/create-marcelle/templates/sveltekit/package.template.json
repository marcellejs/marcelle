  },
  "devDependencies": {
    "@marcellejs/devtools": "workspace:*",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.14",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "vite": "^5.3.1"
  }
}
