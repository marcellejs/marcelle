{
  "name": "@marcellejs/core",
  "description": "Marcelle Core API",
  "version": "0.6.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellejs/marcelle"
  },
  "author": {
    "name": "Jules FrancÌ§oise",
    "email": "me@julesfrancoise.com"
  },
  "keywords": [
    "marcelle",
    "interactive machine learning"
  ],
  "license": "MIT",
  "main": "dist/marcelle.bundle.esm.js",
  "module": "dist/marcelle.esm.js",
  "browser": "dist/marcelle.bundle.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "types": "./types/index.d.ts",
        "import": "./dist/marcelle.esm.js",
        "require": "./dist/marcelle.bundle.umd.js"
      },
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/marcelle.esm.js"
      },
      "require": "./dist/marcelle.bundle.umd.js",
      "default": "./dist/marcelle.esm.js"
    },
    "./dist/marcelle.css": "./dist/marcelle.css"
  },
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "pnpm run clean && rollup -c && pnpm run types:build",
    "bundle": "rollup --config rollup.config.bundle.js",
    "types:dev": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types --watch",
    "types:build": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types",
    "lint": "eslint src",
    "clean": "rimraf dist types"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/ndarray": "^1.0.14",
    "@types/ndarray-ops": "^1.2.7",
    "@types/recordrtc": "^5.6.14",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.50",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rollup": "^4.46.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-svelte": "^7.2.2",
    "svelte": "^5.38.2",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/feathers": "^5.0.34",
    "@feathersjs/memory": "^5.0.34",
    "@feathersjs/socketio-client": "^5.0.34",
    "@kurkle/color": "^0.4.0",
    "auto-bind": "^5.0.1",
    "chart.js": "^4.5.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-chart-matrix": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "dequal": "^2.0.3",
    "luxon": "^3.7.1",
    "ml-kmeans": "^6.0.0",
    "ml-pca": "^4.1.1",
    "recordrtc": "^5.6.2",
    "rxjs": "^7.8.2",
    "scatter-gl": "^0.0.13",
    "scule": "^1.3.0",
    "sift": "^17.1.3",
    "socket.io-client": "^4.8.1",
    "umap-js": "^1.4.0",
    "vidstack": "1.9.8"
  }
