{
  "name": "@marcellejs/gui-widgets",
  "description": "Marcelle component collection of utility GUI widgets",
  "version": "0.6.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellejs/marcelle"
  },
  "author": {
    "name": "Jules FrancÌ§oise",
    "email": "me@julesfrancoise.com"
  },
  "keywords": [
    "marcelle",
    "interactive machine learning"
  ],
  "license": "MIT",
  "main": "dist/marcelle-gui-widgets.bundle.esm.js",
  "module": "dist/marcelle-gui-widgets.esm.js",
  "browser": "dist/marcelle-gui-widgets.bundle.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "types": "./types/index.d.ts",
        "import": "./dist/marcelle-gui-widgets.esm.js",
        "require": "./dist/marcelle-gui-widgets.bundle.umd.js"
      },
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/marcelle-gui-widgets.esm.js"
      },
      "require": "./dist/marcelle-gui-widgets.bundle.umd.js",
      "default": "./dist/marcelle-gui-widgets.esm.js"
    },
    "./dist/marcelle-gui-widgets.css": "./dist/marcelle-gui-widgets.css"
  },
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "pnpm run clean && rollup -c && pnpm run types:build",
    "bundle": "rollup --config rollup.config.bundle.js",
    "types:dev": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types --watch",
    "types:build": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types",
    "lint": "eslint src",
    "clean": "rimraf dist types"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/blueimp-load-image": "^5.16.6",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.50",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rollup": "^4.46.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-svelte": "^7.2.2",
    "svelte": "^5.38.2",
    "svelte-range-slider-pips": "^4.0.7",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@marcellejs/core": "workspace:^0.6.5",
    "blueimp-load-image": "^5.14.0",
    "rxjs": "^7.8.2"
  }
}
